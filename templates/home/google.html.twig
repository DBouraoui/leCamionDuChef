    <!-- JavaScript pour le carousel -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const carousel = document.getElementById('reviews-inner');
            const prevButton = document.getElementById('reviews-prev-btn');
            const nextButton = document.getElementById('reviews-next-btn');
            const indicators = document.querySelectorAll('#reviews-indicators button');

            let currentIndex = 0;
            const itemCount = carousel.children.length;

            // Fonction pour mettre à jour l'affichage du carousel
            function updateCarousel(index) {
                // S'assurer que l'index est dans les limites
                if (index < 0) index = itemCount - 1;
                if (index >= itemCount) index = 0;

                currentIndex = index;

                // Déplacer le carousel
                carousel.style.transform = `translateX(-${currentIndex * 100}%)`;

                // Mettre à jour les indicateurs
                indicators.forEach((indicator, i) => {
                    if (i === currentIndex) {
                        indicator.classList.remove('bg-gray-300');
                        indicator.classList.add('bg-indigo-600');
                    } else {
                        indicator.classList.remove('bg-indigo-600');
                        indicator.classList.add('bg-gray-300');
                    }
                });
            }

            // Écouteurs d'événements pour les boutons
            prevButton.addEventListener('click', () => {
                updateCarousel(currentIndex - 1);
            });

            nextButton.addEventListener('click', () => {
                updateCarousel(currentIndex + 1);
            });

            // Écouteurs d'événements pour les indicateurs
            indicators.forEach((indicator, index) => {
                indicator.addEventListener('click', () => {
                    updateCarousel(index);
                });
            });

            // Initialiser le carousel
            updateCarousel(0);

            // Mettre à jour le lien Google Reviews avec l'URL de votre établissement
            const googleReviewsLink = document.getElementById('google-reviews-link');
            // Remplacez cette URL par celle de votre établissement Google Maps
            googleReviewsLink.href = "https://g.page/r/YOUR-GOOGLE-PLACE-ID/review";
        });
    </script>
</section>